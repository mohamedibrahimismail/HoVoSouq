<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hazem.hovosouq">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.Main">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AllCategories"
            android:parentActivityName=".activity.MainActivity" />

        <activity android:name=".activity.Selected_Category" />
        <activity android:name=".activity.Rating">

        </activity>
        <activity android:name=".activity.Profile">


        </activity>
        <activity
            android:name=".activity.Products"
            android:label="@string/title_activity_products"
            android:theme="@style/Base.Theme" />
        <activity android:name=".activity.ProductDetails"></activity>
        <activity android:name=".activity.SearchActivity"
            android:theme="@style/Base.Theme"></activity>
    </application>

</manifest>